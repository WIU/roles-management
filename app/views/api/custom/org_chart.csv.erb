"uid","givenName","sn","mail","telephoneNumber","street","ou","title","ucdPersonAffiliation","Disabled"
<% @people.each do |person| 
	if person.ous.length > 0
	  ou = ""
	  person.ous.each do |o|
	    ou = ou + o.name
	    unless o == person.ous.last
	      ou = ou + ","
      end
    end
	else
		ou = ""
	end
	
	if person.title
		title = person.title.title
	else
		title = ""
	end
	
	if person.affiliations.length > 0
	  affiliations = ""
	  person.affiliations.each do |a|
	    affiliations = affiliations + a.name
	    unless a == person.affiliations.last
	      affiliations = affiliations + ","
      end
    end
  else
    affiliations = ""
  end
  
  if person.phone
    phone = person.phone
  else
    phone = ""
  end
  
  if person.address
    address = person.address
  else
    address = ""
  end

  if person.email
    email = person.email
  else
    email = ""
  end
%>
"<%= person.loginid.gsub(/\"/, "\\\"").html_safe %>","<%= person.first.gsub(/\"/, "\\\"").html_safe %>","<%= person.last.gsub(/\"/, "\\\"").html_safe %>","<%= email.gsub(/\"/, "\\\"").html_safe %>","<%= phone.gsub(/\"/, "\\\"").html_safe %>","<%= address.gsub(/\"/, "\\\"").html_safe %>","<%= ou.gsub(/\"/, "\\\"").html_safe %>","<%= title.gsub(/\"/, "\\\"").html_safe %>","<%= affiliations.gsub(/\"/, "\\\"").html_safe %>","No"
<% end %>
