"uid","givenName","sn","mail","telephoneNumber","street","ou","title","ucdPersonAffiliation","Disabled"
<% @people.each do |person| 
	if person.ous.length > 0
	  ou = ""
	  person.ous.each do |o|
	    ou = ou + o.name
	    unless o == person.ous.last
	      ou = ou + ","
      end
    end
	else
		ou = ""
	end
	
	if person.title
		title = person.title.title
	else
		title = ""
	end
	
	if person.affiliation
	  affiliation = person.affiliation.name
  else
    affiliation = ""
  end
  
  if person.phone
    phone = person.phone
  else
    phone = ""
  end
  
  if person.address
    address = person.address
  else
    address = ""
  end

  if person.email
    email = person.email
  else
    email = ""
  end
%>
"<%= person.loginid.gsub(/\"/, "\\\"") %>","<%= person.first.gsub(/\"/, "\\\"") %>","<%= person.last.gsub(/\"/, "\\\"") %>","<%= email.gsub(/\"/, "\\\"") %>","<%= phone.gsub(/\"/, "\\\"") %>","<%= address.gsub(/\"/, "\\\"") %>","<%= ou.gsub(/\"/, "\\\"") %>","<%= title.gsub(/\"/, "\\\"") %>","<%= affiliation.gsub(/\"/, "\\\"") %>","No"
<% end %>
